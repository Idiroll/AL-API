{
  "id": "com.yourcompany.illustrator.autonest",
  "name": "Auto Nest Images",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": {
    "app": "AI",
    "minVersion": "26.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "nestingPanel",
      "label": {
        "default": "Auto Nest"
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "icons/plugin-icon.png",
      "scale": [
        1,
        2
      ]
    }
  ],
  "requiredPermissions": [
    "clipboard",
    "allowCodeGenerationFromStrings"
  ]
}
